{
    "batch_size":64,

    "learning_rate": 0.001,

    "activation": "relu",

    "hidden_size": 1,

    "ggn":{
        "enc_layers": 2,
        "gnn_layers": 1,
        "full_graph": true

    },
    "rnngcn":{
        "rnn_layers": 1,
        "gcn_layers": 2,
        "cell_type": "gru",
        "rnn_dropout": 0.0,
        "gcn_dropout": 0.0

    },
    "stcn":{
        "temporal_kernel_size": 3,
        "spatial_kernel_size": 3,
        "n_layers": 3

    },
    "grugcn":{
        "norm": "mean",
        "enc_layers": 2,
        "gcn_layers": 1

    }
}